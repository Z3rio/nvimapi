{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./build",
    "strict": true,
    "moduleResolution": "node",
    "types": [
      "@citizenfx/server",
      "@types/node"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "**/node_modules"
  ]
}